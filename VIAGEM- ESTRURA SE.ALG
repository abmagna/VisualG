Algoritmo "viagem- estrura se"


Var
   dinheiro:inteiro

Inicio
   escreva ("Digite seu saldo: ")
   leia (dinheiro)
   se (dinheiro >=10000) entao
      Escreva ("Partiu disney")
   senao
      se (dinheiro <10000) e (dinheiro>=5000) entao
         escreva ("Partiu BH")
      senao (dinheiro < 10000)
         escreva ("Não foi dessa vez")
      fimse
   fimse


Fimalgoritmo